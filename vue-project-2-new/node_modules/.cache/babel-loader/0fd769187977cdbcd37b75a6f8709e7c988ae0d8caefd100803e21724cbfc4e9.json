{"ast":null,"code":"import { reactive, ref } from \"vue\";\nimport ToggleComponent from \"@/components/ToggleComponent.vue\";\nexport default {\n  components: {\n    ToggleComponent\n  },\n  setup() {\n    const myObject = reactive({\n      property1: \"Привет!\",\n      property2: 42\n    });\n    const updateProperty = () => {\n      myObject.property1 = \"Пока!\";\n      myObject.property2 = 100;\n    };\n    const message = ref(\"Good morning!\");\n    const changeMessage = () => {\n      message.value = \"Goodbye!\";\n    };\n    let userInput = ref(\"\");\n    const submitText = event => {\n      userInput.value = event.target.value;\n    };\n    const items = ref([{\n      id: 1,\n      name: \"яблоко\",\n      type: \"fruit\"\n    }, {\n      id: 2,\n      name: \"морковь\",\n      type: \"vegetable\"\n    }, {\n      id: 3,\n      name: \"апельсин\",\n      type: \"fruit\"\n    }, {\n      id: 4,\n      name: \"капуста\",\n      type: \"vegetable\"\n    }]);\n    const filteredItems = ref(items.value);\n    const filterItems = () => {\n      filteredItems.value = items.value.filter(item => item.type === \"fruit\");\n    };\n    return {\n      myObject,\n      updateProperty,\n      message,\n      changeMessage,\n      submitText,\n      userInput,\n      filteredItems,\n      filterItems,\n      items\n    };\n  }\n};","map":{"version":3,"names":["reactive","ref","ToggleComponent","components","setup","myObject","property1","property2","updateProperty","message","changeMessage","value","userInput","submitText","event","target","items","id","name","type","filteredItems","filterItems","filter","item"],"sources":["C:\\Users\\admin\\Desktop\\js-homework\\vue-project-2\\src\\App.vue"],"sourcesContent":["<template>\n  <div class=\"main-container\">\n    <div class=\"left-content\">\n      <button @click=\"updateProperty\">Поменять значение</button>\n      <p>{{ myObject.property1 }}</p>\n      <p>{{ myObject.property2 }}</p>\n\n      <button @click=\"changeMessage\">Изменить сообщение</button>\n      <p>{{ message }}</p>\n\n      <input type=\"text\" @input=\"submitText\" placeholder=\"Введите текст\">\n      <p>{{ userInput }}</p>\n\n      <button @click=\"filterItems\">Фильтровать элементы</button>\n      <ul>\n        <li v-for=\"item in filteredItems\" :key=\"item.id\">{{item.name}}</li>\n      </ul>\n    </div>\n\n\n    <div class=\"right-content\">\n      <ToggleComponent />\n    </div>\n  </div>\n</template>\n\n<script>\nimport { reactive, ref } from \"vue\";\nimport ToggleComponent from \"@/components/ToggleComponent.vue\";\n\nexport default {\n  components: { ToggleComponent },\n  setup() {\n    const myObject = reactive({\n      property1: \"Привет!\",\n      property2: 42,\n    });\n\n    const updateProperty = () => {\n      myObject.property1 = \"Пока!\";\n      myObject.property2 = 100;\n    };\n\n    const message = ref(\"Good morning!\");\n\n    const changeMessage = () => {\n      message.value = \"Goodbye!\";\n    };\n\n    let userInput = ref(\"\");\n\n    const submitText = (event) => {\n      userInput.value = event.target.value;\n    };\n\n    const items = ref([\n      { id: 1, name: \"яблоко\", type: \"fruit\" },\n      { id: 2, name: \"морковь\", type: \"vegetable\" },\n      { id: 3, name: \"апельсин\", type: \"fruit\" },\n      { id: 4, name: \"капуста\", type: \"vegetable\" },\n    ]);\n\n    const filteredItems = ref(items.value);\n\n    const filterItems = () => {\n      filteredItems.value = items.value.filter(\n          (item) => item.type === \"fruit\"\n      );\n    };\n\n    return {\n      myObject,\n      updateProperty,\n      message,\n      changeMessage,\n      submitText,\n      userInput,\n      filteredItems,\n      filterItems,\n      items,\n    };\n  },\n};\n</script>\n\n<style>\nbutton{\n  background-color: lightpink;\n  color: black;\n  border-radius: 5px;\n}\n.main-container {\n  display: flex;\n  gap: 20px;\n}\n\n.left-content {\n  flex: 1;\n}\n\n.right-content {\n  flex: 1;\n}\n</style>\n"],"mappings":"AA2BA,SAASA,QAAQ,EAAEC,GAAE,QAAS,KAAK;AACnC,OAAOC,eAAc,MAAO,kCAAkC;AAE9D,eAAe;EACbC,UAAU,EAAE;IAAED;EAAgB,CAAC;EAC/BE,KAAKA,CAAA,EAAG;IACN,MAAMC,QAAO,GAAIL,QAAQ,CAAC;MACxBM,SAAS,EAAE,SAAS;MACpBC,SAAS,EAAE;IACb,CAAC,CAAC;IAEF,MAAMC,cAAa,GAAIA,CAAA,KAAM;MAC3BH,QAAQ,CAACC,SAAQ,GAAI,OAAO;MAC5BD,QAAQ,CAACE,SAAQ,GAAI,GAAG;IAC1B,CAAC;IAED,MAAME,OAAM,GAAIR,GAAG,CAAC,eAAe,CAAC;IAEpC,MAAMS,aAAY,GAAIA,CAAA,KAAM;MAC1BD,OAAO,CAACE,KAAI,GAAI,UAAU;IAC5B,CAAC;IAED,IAAIC,SAAQ,GAAIX,GAAG,CAAC,EAAE,CAAC;IAEvB,MAAMY,UAAS,GAAKC,KAAK,IAAK;MAC5BF,SAAS,CAACD,KAAI,GAAIG,KAAK,CAACC,MAAM,CAACJ,KAAK;IACtC,CAAC;IAED,MAAMK,KAAI,GAAIf,GAAG,CAAC,CAChB;MAAEgB,EAAE,EAAE,CAAC;MAAEC,IAAI,EAAE,QAAQ;MAAEC,IAAI,EAAE;IAAQ,CAAC,EACxC;MAAEF,EAAE,EAAE,CAAC;MAAEC,IAAI,EAAE,SAAS;MAAEC,IAAI,EAAE;IAAY,CAAC,EAC7C;MAAEF,EAAE,EAAE,CAAC;MAAEC,IAAI,EAAE,UAAU;MAAEC,IAAI,EAAE;IAAQ,CAAC,EAC1C;MAAEF,EAAE,EAAE,CAAC;MAAEC,IAAI,EAAE,SAAS;MAAEC,IAAI,EAAE;IAAY,CAAC,CAC9C,CAAC;IAEF,MAAMC,aAAY,GAAInB,GAAG,CAACe,KAAK,CAACL,KAAK,CAAC;IAEtC,MAAMU,WAAU,GAAIA,CAAA,KAAM;MACxBD,aAAa,CAACT,KAAI,GAAIK,KAAK,CAACL,KAAK,CAACW,MAAM,CACnCC,IAAI,IAAKA,IAAI,CAACJ,IAAG,KAAM,OAC5B,CAAC;IACH,CAAC;IAED,OAAO;MACLd,QAAQ;MACRG,cAAc;MACdC,OAAO;MACPC,aAAa;MACbG,UAAU;MACVD,SAAS;MACTQ,aAAa;MACbC,WAAW;MACXL;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}