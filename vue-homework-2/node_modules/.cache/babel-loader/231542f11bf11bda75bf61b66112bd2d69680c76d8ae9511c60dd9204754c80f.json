{"ast":null,"code":"import { computed, reactive, ref, watch } from \"vue\";\nimport ToggleComponent from \"@/components/ToggleComponent.vue\";\nexport default {\n  components: {\n    ToggleComponent\n  },\n  setup() {\n    const myObject = reactive({\n      property1: \"Привет!\",\n      property2: 42\n    });\n    const updateProperty = () => {\n      myObject.property1 = \"Пока!\";\n      myObject.property2 = 100;\n    };\n    const message = ref(\"Good morning!\");\n    const changeMessage = () => {\n      message.value = \"Goodbye!\";\n      const apiResponse = ref('');\n    };\n    let userInput = ref(\"\");\n    const submitText = event => {\n      userInput.value = event.target.value;\n    };\n    const items = ref([{\n      id: 1,\n      name: \"яблоко\",\n      type: \"fruit\"\n    }, {\n      id: 2,\n      name: \"морковь\",\n      type: \"vegetable\"\n    }, {\n      id: 3,\n      name: \"апельсин\",\n      type: \"fruit\"\n    }, {\n      id: 4,\n      name: \"капуста\",\n      type: \"vegetable\"\n    }]);\n    const filteredItems = ref(items.value);\n    const filterItems = () => {\n      filteredItems.value = items.value.filter(item => item.type === \"fruit\");\n    };\n    const basePrice = ref(100);\n    const taxRate = ref(15);\n    const discount = ref(10);\n    const priceWithTax = computed(() => {\n      return basePrice.value * (1 + taxRate.value);\n    });\n    const finalPrice = computed(() => {\n      return priceWithTax.value - discount.value;\n    });\n    watch(() => myObject.property1, (newVal, oldVal) => {\n      console.log(`property1 изменился с \"${oldVal}\" на \"${newVal}\"`);\n    });\n    watch(() => message.value, (newVal, oldVal) => {\n      console.log(`message изменилось с \"${oldVal}\" на \"${newVal}\"`);\n    });\n    watch(() => message.value, async (newVal, oldVal) => {\n      console.log(`message изменилось с \"${oldVal}\" на \"${newVal}\"`);\n      // Выполняем API-запрос при изменении message\n      try {\n        const response = await fetch(`https://jsonplaceholder.typicode.com/posts/${newVal.length}`);\n        const data = await response.json();\n        apiResponse.value = data.title;\n      } catch (error) {\n        console.error(\"Ошибка при выполнении API-запроса:\", error);\n      }\n    });\n    return {\n      myObject,\n      updateProperty,\n      message,\n      changeMessage,\n      submitText,\n      userInput,\n      filteredItems,\n      filterItems,\n      items,\n      basePrice,\n      taxRate,\n      discount,\n      priceWithTax,\n      finalPrice,\n      apiResponse\n    };\n  }\n};","map":{"version":3,"names":["computed","reactive","ref","watch","ToggleComponent","components","setup","myObject","property1","property2","updateProperty","message","changeMessage","value","apiResponse","userInput","submitText","event","target","items","id","name","type","filteredItems","filterItems","filter","item","basePrice","taxRate","discount","priceWithTax","finalPrice","newVal","oldVal","console","log","response","fetch","length","data","json","title","error"],"sources":["C:\\Users\\admin\\Desktop\\js-homework\\vue-project-2\\src\\App.vue"],"sourcesContent":["<template>\n  <div class=\"main-container\">\n    <div class=\"left-content\">\n      <button @click=\"updateProperty\">Поменять значение</button>\n      <p>{{ myObject.property1 }}</p>\n      <p>{{ myObject.property2 }}</p>\n\n      <button @click=\"changeMessage\">Изменить сообщение</button>\n      <p>{{ message }}</p>\n      <h3>Ответ API: {{apiResponse}}</h3>\n\n      <input type=\"text\" @input=\"submitText\" placeholder=\"Введите текст\">\n      <p>{{ userInput }}</p>\n\n      <button @click=\"filterItems\">Фильтровать элементы</button>\n      <ul>\n        <li v-for=\"item in filteredItems\" :key=\"item.id\">{{item.name}}</li>\n      </ul>\n\n      <h3>Цена с налогом: {{priceWithTax}}</h3>\n      <h3>Окончательная цена: {{finalPrice}}</h3>\n    </div>\n\n\n    <div class=\"right-content\">\n      <ToggleComponent />\n    </div>\n  </div>\n</template>\n\n<script>\nimport {computed, reactive, ref, watch} from \"vue\";\nimport ToggleComponent from \"@/components/ToggleComponent.vue\";\n\nexport default {\n  components: { ToggleComponent },\n  setup() {\n    const myObject = reactive({\n      property1: \"Привет!\",\n      property2: 42,\n    });\n\n    const updateProperty = () => {\n      myObject.property1 = \"Пока!\";\n      myObject.property2 = 100;\n    };\n\n    const message = ref(\"Good morning!\");\n\n    const changeMessage = () => {\n      message.value = \"Goodbye!\";\n      const apiResponse = ref ('');\n    };\n\n    let userInput = ref(\"\");\n\n    const submitText = (event) => {\n      userInput.value = event.target.value;\n    };\n\n    const items = ref([\n      { id: 1, name: \"яблоко\", type: \"fruit\" },\n      { id: 2, name: \"морковь\", type: \"vegetable\" },\n      { id: 3, name: \"апельсин\", type: \"fruit\" },\n      { id: 4, name: \"капуста\", type: \"vegetable\" },\n    ]);\n\n    const filteredItems = ref(items.value);\n\n    const filterItems = () => {\n      filteredItems.value = items.value.filter(\n          (item) => item.type === \"fruit\"\n      );\n    };\n    const basePrice = ref (100);\n    const taxRate = ref(15);\n    const discount = ref(10);\n    const priceWithTax = computed(() =>{\n      return basePrice.value * (1 + taxRate.value);\n    });\n    const finalPrice = computed(() =>{\n      return priceWithTax.value - discount.value;\n    });\n\n    watch(\n        () => myObject.property1,\n        (newVal, oldVal) => {\n          console.log(`property1 изменился с \"${oldVal}\" на \"${newVal}\"`);\n        }\n    );\n    watch(\n        () => message.value,\n        (newVal, oldVal) => {\n          console.log(`message изменилось с \"${oldVal}\" на \"${newVal}\"`);\n        }\n    );\n    watch(\n        () => message.value,\n        async (newVal, oldVal) => {\n          console.log(`message изменилось с \"${oldVal}\" на \"${newVal}\"`);\n          // Выполняем API-запрос при изменении message\n          try {\n            const response = await fetch(`https://jsonplaceholder.typicode.com/posts/${newVal.length}`);\n            const data = await response.json();\n            apiResponse.value = data.title;\n          } catch (error) {\n            console.error(\"Ошибка при выполнении API-запроса:\", error);\n          }\n        }\n    );\n\n\n\n\n\n    return {\n      myObject,\n      updateProperty,\n      message,\n      changeMessage,\n      submitText,\n      userInput,\n      filteredItems,\n      filterItems,\n      items,\n      basePrice,\n      taxRate,\n      discount,\n      priceWithTax,\n      finalPrice,\n      apiResponse\n    };\n  },\n};\n</script>\n\n<style>\nbutton{\n  background-color: lightpink;\n  color: black;\n  border-radius: 5px;\n}\n.main-container {\n  display: flex;\n  gap: 20px;\n}\n\n.left-content {\n  flex: 1;\n}\n\n.right-content {\n  flex: 1;\n}\n</style>\n"],"mappings":"AA+BA,SAAQA,QAAQ,EAAEC,QAAQ,EAAEC,GAAG,EAAEC,KAAK,QAAO,KAAK;AAClD,OAAOC,eAAc,MAAO,kCAAkC;AAE9D,eAAe;EACbC,UAAU,EAAE;IAAED;EAAgB,CAAC;EAC/BE,KAAKA,CAAA,EAAG;IACN,MAAMC,QAAO,GAAIN,QAAQ,CAAC;MACxBO,SAAS,EAAE,SAAS;MACpBC,SAAS,EAAE;IACb,CAAC,CAAC;IAEF,MAAMC,cAAa,GAAIA,CAAA,KAAM;MAC3BH,QAAQ,CAACC,SAAQ,GAAI,OAAO;MAC5BD,QAAQ,CAACE,SAAQ,GAAI,GAAG;IAC1B,CAAC;IAED,MAAME,OAAM,GAAIT,GAAG,CAAC,eAAe,CAAC;IAEpC,MAAMU,aAAY,GAAIA,CAAA,KAAM;MAC1BD,OAAO,CAACE,KAAI,GAAI,UAAU;MAC1B,MAAMC,WAAU,GAAIZ,GAAE,CAAG,EAAE,CAAC;IAC9B,CAAC;IAED,IAAIa,SAAQ,GAAIb,GAAG,CAAC,EAAE,CAAC;IAEvB,MAAMc,UAAS,GAAKC,KAAK,IAAK;MAC5BF,SAAS,CAACF,KAAI,GAAII,KAAK,CAACC,MAAM,CAACL,KAAK;IACtC,CAAC;IAED,MAAMM,KAAI,GAAIjB,GAAG,CAAC,CAChB;MAAEkB,EAAE,EAAE,CAAC;MAAEC,IAAI,EAAE,QAAQ;MAAEC,IAAI,EAAE;IAAQ,CAAC,EACxC;MAAEF,EAAE,EAAE,CAAC;MAAEC,IAAI,EAAE,SAAS;MAAEC,IAAI,EAAE;IAAY,CAAC,EAC7C;MAAEF,EAAE,EAAE,CAAC;MAAEC,IAAI,EAAE,UAAU;MAAEC,IAAI,EAAE;IAAQ,CAAC,EAC1C;MAAEF,EAAE,EAAE,CAAC;MAAEC,IAAI,EAAE,SAAS;MAAEC,IAAI,EAAE;IAAY,CAAC,CAC9C,CAAC;IAE