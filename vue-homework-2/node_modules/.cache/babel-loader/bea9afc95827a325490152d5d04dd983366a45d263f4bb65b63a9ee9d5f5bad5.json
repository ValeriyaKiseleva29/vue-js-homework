{"ast":null,"code":"import { reactive, ref } from \"vue\";\nimport ToggleComponent from \"@/components/ToggleComponent.vue\";\nexport default {\n  components: {\n    ToggleComponent\n  },\n  setup() {\n    const myObject = reactive({\n      property1: 'Привет!',\n      property2: 42\n    });\n    const updateProperty = () => {\n      myObject.property1 = 'Пока!';\n      myObject.property2 = 100;\n    };\n    const message = ref(\"Good morning!\");\n    const changeMessage = () => {\n      message.value = \"Goodbye!\";\n    };\n    let userInput = ref('');\n    const submitText = event => {\n      userInput.value = event.target.value;\n    };\n    const items = ref([{\n      id: 1,\n      name: 'яблоко',\n      type: 'fruit'\n    }, {\n      id: 2,\n      name: 'морковь',\n      type: 'vegateble'\n    }, {\n      id: 3,\n      name: 'апельсин',\n      type: 'fruit'\n    }, {\n      id: 4,\n      name: 'капуста',\n      type: 'vegateble'\n    }]);\n    const filteredItems = ref(items.value);\n    const filterItems = () => {\n      filteredItems.value = items.value.filter(item => item.type === 'fruit');\n    };\n    return {\n      myObject,\n      updateProperty,\n      message,\n      changeMessage,\n      submitText,\n      userInput,\n      filteredItems,\n      filterItems,\n      items\n    };\n  }\n};","map":{"version":3,"names":["reactive","ref","ToggleComponent","components","setup","myObject","property1","property2","updateProperty","message","changeMessage","value","userInput","submitText","event","target","items","id","name","type","filteredItems","filterItems","filter","item"],"sources":["C:\\Users\\admin\\Desktop\\js-homework\\vue-project-2\\src\\App.vue"],"sourcesContent":["<template>\n<div>\n  <ToggleComponent/>\n<button @click=\"updateProperty\">Поменять значение</button>\n  <p>{{myObject.property1}}</p>\n  <p>{{myObject.property2}}</p>\n\n  <button @click=\"changeMessage\">Изменить сообщение</button>\n  <p>{{message}}</p>\n\n  <input type=\"text\" @input=\"submitText\" placeholder=\"Введите текст\">\n  <p>{{userInput}}</p>\n\n  <button @click=\"filterItems\">Фильтровать эллементы</button>\n  <ul>\n    <li v-for=\"item in filteredItems\" :key=\"item.id\">{{item.name}}</li>\n  </ul>\n</div>\n</template>\n\n<script>\nimport {reactive, ref} from \"vue\";\nimport ToggleComponent from \"@/components/ToggleComponent.vue\";\n export default {\n   components: {ToggleComponent},\n   setup(){\n     const myObject = reactive({\n       property1: 'Привет!',\n       property2: 42\n     });\n     const updateProperty = () => {\n       myObject.property1 = 'Пока!';\n       myObject.property2 = 100\n     };\n     const message = ref(\"Good morning!\");\n     const changeMessage = () => {\n       message.value = \"Goodbye!\"\n     };\n     let userInput = ref('');\n     const submitText = (event) => {\n       userInput.value = event.target.value;\n     };\n     const items = ref([\n       {id: 1, name: 'яблоко', type:'fruit' },\n       {id: 2, name: 'морковь', type:'vegateble' },\n       {id: 3, name: 'апельсин', type:'fruit' },\n       {id: 4, name: 'капуста', type:'vegateble' }\n\n     ]);\n        const filteredItems = ref(items.value);\n        const filterItems = () => {\n          filteredItems.value = items.value.filter((item) => item.type === 'fruit');\n        };\n\n     return{\n       myObject,\n       updateProperty,\n       message,\n       changeMessage,\n       submitText,\n       userInput,\n       filteredItems,\n       filterItems,\n       items\n     };\n   }\n };\n</script>\n\n<style>\n button{\n  background-color: lightpink;\n   color: black;\n  border-radius: 5px;\n}\n\n</style>\n"],"mappings":"AAqBA,SAAQA,QAAQ,EAAEC,GAAG,QAAO,KAAK;AACjC,OAAOC,eAAc,MAAO,kCAAkC;AAC7D,eAAe;EACbC,UAAU,EAAE;IAACD;EAAe,CAAC;EAC7BE,KAAKA,CAAA,EAAE;IACL,MAAMC,QAAO,GAAIL,QAAQ,CAAC;MACxBM,SAAS,EAAE,SAAS;MACpBC,SAAS,EAAE;IACb,CAAC,CAAC;IACF,MAAMC,cAAa,GAAIA,CAAA,KAAM;MAC3BH,QAAQ,CAACC,SAAQ,GAAI,OAAO;MAC5BD,QAAQ,CAACE,SAAQ,GAAI,GAAE;IA